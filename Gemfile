# To install, run `bundle install`.
# To upgrade, run `bundle update`.
# Run Jekyll with `bundle exec jekyll serve`.

source 'https://rubygems.org'

# gem "github-pages"
gem "jekyll", "~> 4.2"

group :jekyll_plugins do
  gem 'jekyll-optional-front-matter'
  gem "jekyll-sitemap"
  gem "jekyll-feed"
  gem "jekyll-seo-tag"
  gem "jekyll-mentions"
  gem "jekyll-avatar"
  gem 'jekyll-wikirefs'
  gem "kramdown"
  gem "minima"
end
